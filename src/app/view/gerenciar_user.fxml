<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.UsersController">
   <center>
      <GridPane alignment="CENTER" prefHeight="538.0" prefWidth="737.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <BorderPane.margin>
            <Insets bottom="80.0" left="80.0" right="80.0" top="80.0" />
         </BorderPane.margin>
         <children>
            <Button fx:id="new_func_button" mnemonicParsing="false" onAction="#goAddUsers" prefHeight="156.0" prefWidth="320.0" text="Cadastrar Novo Usuario">
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin></Button>
            <Button fx:id="gerenciar_func_button" mnemonicParsing="false" onAction="#goDadosUsers" prefHeight="164.0" prefWidth="418.0" text="Gerenciar Usuarios" GridPane.columnIndex="1">
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin></Button>
         </children>
      </GridPane>
   </center>
</BorderPane>
